{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Biały",
					"slug": "white"
				},
				{
					"color": "#191919",
					"name": "Czarny",
					"slug": "black"
				},
				{
					"color": "#EEEEEE",
					"name": "Szary",
					"slug": "grey"
				},
				{
					"color": "#009198",
					"name": "Główny",
					"slug": "primary"
				},
				{
					"color": "#E6F4F5",
					"name": "Główny 10%",
					"slug": "primary-light"
				},
				{
					"color": "#EDC74C",
					"name": "Akcent",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": [ "%", "px", "em", "rem", "vh", "vw" ],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "20px",
					"size": "1.25rem",
					"slug": "10"
				},
				{
					"name": "12/25px",
					"size": "clamp(0.75rem, 0.1696rem + 1.8571vw, 1.5625rem)",
					"slug": "20"
				},
				{
					"name": "25/50px",
					"size": "clamp(1.5625rem, 0.4464rem + 3.5714vw, 3.125rem)",
					"slug": "30"
				},
				{
					"name": "37/75px",
					"size": "clamp(2.3125rem, 0.6161rem + 5.4286vw, 4.6875rem)",
					"slug": "40"
				},
				{
					"name": "50/100px",
					"size": "clamp(3.125rem, 0.8929rem + 7.1429vw, 6.25rem)",
					"slug": "50"
				},
				{
					"name": "62/125px",
					"size": "clamp(3.875rem, 1.0625rem + 9vw, 7.8125rem)",
					"slug": "60"
				},
				{
					"name": "75/150px",
					"size": "clamp(4.6875rem, 1.3393rem + 10.7143vw, 9.375rem)",
					"slug": "70"
				},
				{
					"name": "87/175px",
					"size": "clamp(5.4375rem, 1.5089rem + 12.5714vw, 10.9375rem)",
					"slug": "80"
				},
				{
					"name": "100/200px",
					"size": "clamp(6.25rem, 1.7857rem + 14.2857vw, 12.5rem)",
					"slug": "90"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"letterSpacing": false,
			"fluid": {
				"maxViewportWidth": "1200px",
				"minViewportWidth": "320px"
			},
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontWeight": "300",
							"src": [
								"file:./build/fonts/lato-v24-latin-ext-300.woff2"
							]
						},
						{
							"fontFamily": "Lato",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontWeight": "400",
							"src": [
								"file:./build/fonts/lato-v24-latin-ext-regular.woff2"
							]
						},
						{
							"fontFamily": "Lato",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontWeight": "700",
							"src": [
								"file:./build/fonts/lato-v24-latin-ext-700.woff2"
							]
						}
					],
					"fontFamily": "Lato",
					"name": "Lato",
					"slug": "primary"
				}
			],
			"fontSizes": [
				{
					"slug": "text",
					"fluid": {
						"min": "1rem",
						"max": "1rem"
					},
					"size":  "1rem",
					"name": "Tekst"
				},
				{
					"slug": "h4",
					"fluid": {
						"min": "1.2rem",
						"max": "1.5625rem"
					},
					"size": "1.5625rem",
					"name": "H4"
				},
				{
					"slug": "h3",
					"fluid": {
						"min": "1.3rem",
						"max": "2.1875rem"
					},
					"size": "2.1875rem",
					"name": "H3"
				},
				{
					"slug": "h2",
					"fluid": {
						"min": "1.5rem",
						"max": "2.8125rem"
					},
					"size": "2.8125rem",
					"name": "H2"
				},
				{
					"slug": "h1",
					"fluid": {
						"min": "2rem",
						"max": "3.4375rem"
					},
					"size": "3.4375rem",
					"name": "H1"
				}
			]
		},
		"custom": {
			"layout": {
				"padding": "20px",
				"col-padding": "10px"
			},
			"animation": {
				"curve": "cubic-bezier(.2,.75,.5,1)",
				"duration": ".2s"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-1)",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)",
					"fontWeight": "700"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-3)",
					"fontWeight": "700"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-4)",
					"fontWeight": "700"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "400"
				}
			}
		},
		"spacing": {
			"blockGap": "0px"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--text)",
			"lineHeight": "1.2"
		}
	}
}
